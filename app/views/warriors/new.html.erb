<%= simple_form_for [current_user, @warrior] do |f| %>
  <%= f.input :nickname %>
  <%= f.input :specialty %>
  <%= f.input :race %>
  <%= f.input :weapon %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Add a warrior", class: "btn btn-primary" %>
<% end %>
